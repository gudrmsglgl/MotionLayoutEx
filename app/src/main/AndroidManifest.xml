<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cleannote.motionex">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MotionEx">
        <activity android:name=".cover.CoverEx1Activity"></activity>
        <activity android:name=".cover.CoverActivity" />
        <activity android:name=".toggle.MotionToggleActivity" />
        <activity android:name=".swipe.MotionSwipeActivity" />
        <activity android:name=".card.CardActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>